<%- include('header'); -%>
    <div class="box" id="heading">
        <h1><%=kindOfTitle%></h1>
    </div>


    <div class="box">

        
            <% for (let i=0;i<newItemLists.length;i++){ %>
            <div class="item">
                <input type="checkbox">
                <p><%=newItemLists[i]%></p>
            </div>
            <%}%>

            <form class="item" action="/" method="POST">
                <input type="text" name="newItem" placeholder="New Item" autocomplete="off">
                <button type="submit" name="list" value=<%=kindOfTitle%>>+</button>
            </form>
        

    </div>

    <%- include('footer'); -%>
